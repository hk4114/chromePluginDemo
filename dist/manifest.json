{
    "manifest_version": 2,
    "name": "Covid19",
    "version": "1.0.0",
    "description": "Provides the cases details regarding Covid19 with respective to any Country",
    "browser_action": {
      "default_popup": "index.html"
    },
    "icons":{
      "16": "./icons/hero.jpg",
      "64": "./icons/hero.jpg",
     "128": "./icons/hero.jpg"
    },

    "content_security_policy": "script-src 'self' https://unpkg.com ; object-src 'self'"
}
